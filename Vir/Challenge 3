import random
myrandom=random.randint(1,100)
#print(myrandom)

while True:
  userguess=input("Enter a number between 1 and 100: ")
  if myrandom==int(userguess):
    print("You Won!!")
    break
  elif myrandom > int(userguess):
    print("You guessed low")
  else:
    print("You guessed high")

  useryesno=input("You want to guess again? y/n")
  if useryesno=="n":
    break
